class nestedtry 
{
public static void main(String args[]) 
{
try 
{
try 
{
int data=100/0;
} 
catch(Exception e) 
{
System.out.println(e);
}
try
{
int[] a={1,2,3,4,5};
System.out.println(a[10]);
}
catch(Exception e) 
{
System.out.println(e);
} 
}
finally 
{
System.out.println("Example of nested try");
} }}
